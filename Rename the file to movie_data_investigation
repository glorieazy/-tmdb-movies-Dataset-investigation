   "outputs": [],
   "source": [
    "#i used 0.1 here because there columns with 0 returns. so i classify them as loss\n",
    "\n",
    "returns = [-413912431, 0.1, 2544505847]\n",
    "pnl = ['Loss', 'Profit']\n",
    "df['profit_or_loss'] = pd.cut(df['returns'], returns, labels=pnl)\n"
   ]
  },
